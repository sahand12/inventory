<!-- color, id, date, title, categoryName, amount, user -->
<script type="text/vgn-template" id="adminExpensesTableTemplate">
    <tr>
        <td class="category-color" style="background-color: [[color]]"></td>
        <td class="text-center">
            <div class="description-popover" tabindex="0" role="button" data-toggle="popover" data-trigger="focus" data-content="[[description]]">
                <span class="fa fa-file-text-o text-center"></span>
            </div>
        </td>
        <td>[[date]]</td>
        <td>[[title]]</td>
        <td class="text-capitalize"><a href="/cost/admin/users/[[userId]]/expenses">[[username]]</a></td>
        <td class="text-capitalize">[[categoryName]]</td>
        <td class="expense-table-amount">[[amount]]</td>
    </tr>
</script>